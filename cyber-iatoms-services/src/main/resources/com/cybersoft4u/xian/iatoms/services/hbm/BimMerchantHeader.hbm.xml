<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cybersoft4u.xian.iatoms.services.dmo.BimMerchantHeader" table="BIM_MERCHANT_HEADER" schema="dbo">
        <id name="merchantHeaderId" type="string">
            <column name="MERCHANT_HEADER_ID" length="32" />
            <generator class="assigned" />
        </id>
        <property name="isVip" type="string">
            <column name="IS_VIP" not-null="true"  length="1"/>
        </property>
        <property name="merchantId" type="string">
            <column name="MERCHANT_ID" not-null="true" length="32"/>
        </property>
        <property name="headerName" type="string">
            <column name="HEADER_NAME" />
        </property>
        <property name="area" type="string">
            <column name="AREA" length="40" />
        </property>
        <property name="contact" type="string">
            <column name="CONTACT" />
        </property>
        <property name="contactTel" type="string">
            <column name="CONTACT_TEL" />
        </property>
        <property name="contactTel2" type="string">
            <column name="CONTACT_TEL2" />
        </property>
        <property name="contactEmail" type="string">
            <column name="CONTACT_EMAIL" />
        </property>
        <property name="phone" type="string">
            <column name="PHONE" />
        </property>
        <property name="location" type="string">
            <column name="LOCATION" length="40" />
        </property>
        <property name="businessAddress" type="string">
            <column name="BUSINESS_ADDRESS" />
        </property>
        <property name="openHour" type="string">
            <column name="OPEN_HOUR" />
        </property>
        <property name="closeHour" type="string">
            <column name="CLOSE_HOUR" />
        </property>
        <property name="aoName" type="string">
            <column name="AO_NAME" />
        </property>
        <property name="aoemail" type="string">
            <column name="AOEmail" />
        </property>
        <property name="remark" type="string">
            <column name="REMARK" />
        </property>
        <property name="createdById" type="string">
            <column name="CREATED_BY_ID" />
        </property>
        <property name="createdByName" type="string">
            <column name="CREATED_BY_NAME" />
        </property>
        <property name="createdDate" type="timestamp">
            <column name="CREATED_DATE" length="23" />
        </property>
        <property name="updatedById" type="string">
            <column name="UPDATED_BY_ID" />
        </property>
        <property name="updatedByName" type="string">
            <column name="UPDATED_BY_NAME" />
        </property>
        <property name="updatedDate" type="timestamp">
            <column name="UPDATED_DATE" length="23" />
        </property>
        <property name="deleted" type="string">
            <column name="DELETED" length="1" />
        </property>
        <property name="postCodeId" type="string">
            <column name="POST_CODE_ID" length="30" />
        </property>
    </class>
</hibernate-mapping>
