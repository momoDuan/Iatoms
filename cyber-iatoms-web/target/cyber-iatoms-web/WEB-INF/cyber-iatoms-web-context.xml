			<ref bean="jobExportCaseOverdue" />
		</property>
		<property name="cronExpression">
			<value>0 0 */1 * * ?</value>	<!--  每隔一小時執行一次  -->
		</property>
	</bean>
	<bean id="jobExportCaseOverdue" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">
		<property name="targetObject">
			<ref bean="exportCaseOverdueServiceBean" />
		</property>
		<property name="targetMethod">
			<value>queryCaseInfo</value>
		</property>
