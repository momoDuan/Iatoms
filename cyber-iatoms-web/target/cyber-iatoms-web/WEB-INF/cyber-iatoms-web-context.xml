        	<value>queryCaseInfo</value>
    	</property>
	</bean>
	<bean id="queryCaseInfo" class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
		<property name="jobDetail">
		    <ref bean="queryCmsCase"/>
		</property>
		<property name="cronExpression">
		    <value>00 3/30 * * * ?</value> <!-- 每 30分鐘 触发一次 -->
		</property>
	</bean>
	<!-- (CMS)_批次作業(匯出案件資料) end-->
	<!-- (CMS)_批次作業(設備停用、啟用) start-->
	<bean id="queryCmsAsset" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">
    	<!-- 调用的类 -->
    	<property name="targetObject">
        	<ref bean="assetManageServiceBean"/>
    	</property>
   		 <!-- 调用类中的方法 -->
    	<property name="targetMethod">
        	<value>queryAssetInfo</value>
    	</property>
	</bean>
	<bean id="queryAssetInfo" class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
		<property name="jobDetail">
		    <ref bean="queryCmsAsset"/>
		</property>
		<!-- cron表达式 -->
		<property name="cronExpression">
		    <value>00 0/30 * * * ?</value> <!-- 每 30分鐘 触发一次 -->
		</property>
	</bean>
	<!-- (CMS)_批次作業(設備停用、啟用) end-->
	<!-- 匯出案件資訊範本(派工給華經資訊)  Start (nicklin) -->
	<bean id="exportCaseTemplateForFis"
