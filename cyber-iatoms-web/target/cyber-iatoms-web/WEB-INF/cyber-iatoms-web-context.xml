				<ref bean="edcFeeReportForScsb"/><!--維護費報表(上銀格式Scsb) -->
				<ref bean="dealSystemLimit"/><!--批次處理 系統備份或刪除 -->
				<ref bean="edcFeeReportForSyb"/><!--維護費報表(陽信格式Syb-Edc) -->
				<ref bean="edcFeeReportForChb"/><!--維護費報表(彰銀格式Chb-Edc)-->
				<ref bean="queryCaseInfo"/><!--CMS)_批次作業(匯出案件資料)-->
				<ref bean="queryAssetInfo"/>  <!--(CMS)_批次作業(設備停用、啟用)-->
				<ref bean="exportCaseTemplateForFis"/><!--匯出案件資訊範本(第一次派工給華經資訊)-->
				<ref bean="exportCaseOverdue"/><!--批次通知工程師回應、到場、完修逾期情況-->
			</list>
		</property>
	</bean>
</beans>