			<ref bean="jobExportCaseTemplateForFis" />
		</property>
		<property name="cronExpression">
			<value>00 30 23 * * ?</value>	<!--  每日23:30  -->
		</property>
	</bean>
	<bean id="jobExportCaseTemplateForFis"
		class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">
		<property name="targetObject">
			<ref bean="exportCaseTemplateForFisServiceBean" />
		</property>
		<property name="targetMethod">
			<value>exportCaseTemplate</value>
		</property>
	</bean>
	 <!-- 匯出案件資訊範本(派工給華經資訊)  End (nicklin) -->
	 
	<!-- 批次通知工程師回應、到場、完修逾期情況  Start (CarrieDuan) -->
	<bean id="exportCaseOverdue" class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
